<?xml version='1.0' encoding='UTF-8' ?>
<nav class="indexNav">
    <ul class="indexNav-D1List" ctrl-for="this.i=0;this.i<this.data.d1List.length;++this.i">
        <li class="indexNav-D1Item hiddenChile">
            <div class="indexNav-D1Content">
                <a class="indexNav-D1Link" href="${this.data.d1List[this.i].url}" title="${this.data.d1List[this.i].title}">
                    ${this.data.d1List[this.i].text}
                </a>
            </div>
            <div ctrl-if="this.data.d1List[this.i].child&&this.data.d1List[this.i].child.length" class="indexNav-D2ListBox">
                <ul class="indexNav-D2List" ctrl-for="this.j=0;this.j<this.data.d1List[this.i].child.length;++this.j">
                    <li class="indexNav-D2Item">
                        <div class="indexNav-D2Content">
                            <a class="indexNav-D2Link" href="${this.data.d1List[this.i].child[this.j].url}" title="${this.data.d1List[this.i].child[this.j].title}">
                                ${this.data.d1List[this.i].child[this.j].text}
                            </a>
                        </div>
                    </li>
                </ul>
            </div>
        </li>
    </ul>
</nav>

<ctrl_tab/>

<div class="audioControl" ctrl-id="root">
    <audio ctrl="audioControl-audioTag" ctrl-id="audioTag"></audio>
    <div class="audioControl-CtrlBox">
        <div class="audioControl-visual">
            <div ctrl-id="spectrumBox" class="audioControl-spectrumBox">
                <img ctrl="image" class="audioControl-image" />
            </div>
            <div class="audioControl-playBar">
                <div class="audioControl-playBarLow" ctrlid="playBarLow" ></div>
                <div class="audioControl-playBarBtn" ctrlid="playBarBtn" pa-mousedown="this.radeyChangCurrentTime()"></div>
            </div>
            <div class="audioControl-volumeBar">
                <div class="audioControl-volumeBarPower"    ctrl-id="volumeBarPower"></div>
                <div class="audioControl-volumeBarBtn"      ctrl-id="volumeBarBtn"  pa-mousedown="this.radeyChangVolume()"></div>
            </div>
        </div>
        <div class="audioControl-buttons">
            <div class="audioControl-button audioControl-playType order"  pa-click="this.setPlayType(this.playType+1);" ctrl-id="playType" title="order">playType</div>
            <div class="audioControl-button audioControl-last"      pa-click="this.last()" ctrl-id="last">last</div>
            <div class="audioControl-button audioControl-playPause play" pa-click="this.playPause()" ctrl-id="playPause">play or pause</div>
            <div class="audioControl-button audioControl-next"      pa-click="this.next();" ctrl-id="next">next</div>
            <div class="audioControl-button audioControl-callList"  pa-click="this.callList();" ctrl-id="callList">callList</div>
        </div>
        <div class="audioControl-medioListBox" ctrl-id="medioListBox" style="visibility:${this.medioListBoxVis?'visible':'hidden'}; display:${this.medioListBoxVis?'block':'none'};">

            <ul ctrl-id="medioList" class="audioControl-medioList" ctrl-for="this.i=0;this.i<this.data.medioList.length;this.i++">
                <li ctrl-id="medioItem" class="audioControl-medioItem">
                    <i class="audioControl-medioItemIndex">${this.i}</i>&nbsp;&nbsp;
                    <a class="audioControl-medioItemLink" pa-click="stopPE(e);this.setPlayingIndex(${this.i});" title="${this.data.medioList[this.i].title}" href="${this.data.medioList[this.i].url}">
                        ${this.data.medioList[this.i].title}
                    </a>
                    <button class="audioControl-medioItemButton hidderText" pa-click="this.removeItem(${this.i})">remove</button>
                </li>
            </ul>
        </div>
    </div>
</div>